{
  "name": "food-express",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch . --ext ts,json --exec tsx bin/www.ts",
    "build": "tsc",
    "test": "node --loader ts-node/esm ./node_modules/mocha/bin/mocha.js test/*.test.ts --timeout 20000",
    "test:coverage": "c8 npm test",
    "coverage:report": "c8 report --reporter=html --reporter=text",
    "coverage:json": "c8 report --reporter=json",
    "clean": "powershell -Command \"Remove-Item -Recurse -Force coverage, .nyc_output, dist -ErrorAction SilentlyContinue\""
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "3.1.10",
    "express": "^5.0.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.20.0",
    "mongoose": "^8.19.0",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/chai": "^5.2.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mocha": "^10.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.6.2",
    "@types/supertest": "^6.0.3",
    "c8": "^10.1.3",
    "chai": "^6.2.0",
    "dotenv": "^17.2.3",
    "mocha": "^11.7.4",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
