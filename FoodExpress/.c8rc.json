{
  "all": true,
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "test/**",
    "node_modules/**",
    "coverage/**",
    "bin/**",
    "dist/**",
    "**/*.d.ts",
    "**/*.test.ts",
    "**/*.spec.ts"
  ],
  "reporter": [
    "text",
    "html",
    "json"
  ],
  "reportsDir": "coverage",
  "checkCoverage": false,
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80
}